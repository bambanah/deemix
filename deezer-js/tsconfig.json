{
	"compilerOptions": {
		/* https://aka.ms/tsconfig */
		"target": "es2016",
		"module": "commonjs",
		"rootDir": "./src",
		"allowJs": true,
		"checkJs": true,
		"outDir": "./dist",
		"noEmit": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": false,
		"skipLibCheck": true
	}
}
