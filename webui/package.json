{
	"name": "deemix-webui",
	"version": "4.0.2",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "nodemon",
		"build": "run-s build:*",
		"build:1-client": "vite build",
		"build:2-server": "tsc --build src/server/tsconfig.json",
		"build:3-aliases": "tsc-alias -p src/server/tsconfig.json",
		"start": "cross-env NODE_ENV=production tsx --tsconfig ./src/server/tsconfig.json src/server/main.ts",
		"test": "jest",
		"test-watch": "jest --watch",
		"testlang": "node ./tests/testlang.js",
		"lint:ts": "run-p lint:server",
		"lint:client": "vue-tsc --noEmit -p src/client/tsconfig.json",
		"lint:server": "tsc --noEmit -p src/server/tsconfig.json"
	},
	"nodemonConfig": {
		"watch": [
			"src/server",
			"../deemix/dist",
			"../deezer-js/dist"
		],
		"ext": "ts,json,js",
		"exec": "tsx --tsconfig ./src/server/tsconfig.json src/server/main.ts",
		"tsconfig": "./src/server/tsconfig.json"
	},
	"exports": {
		".": "./dist/main.js"
	},
	"dependencies": {
		"cookie-parser": "1.4.6",
		"cross-env": "^7.0.3",
		"date-fns": "^3.6.0",
		"debug": "2.6.9",
		"deemix": "workspace:*",
		"deezer-js": "workspace:*",
		"dotenv": "~16.4.5",
		"express": "^4.19.2",
		"express-session": "^1.17.3",
		"flag-icon-css": "3.5.0",
		"got": "^11.8.2",
		"lodash-es": "4.17.21",
		"memorystore": "1.6.7",
		"mitt": "^3.0.1",
		"morgan": "1.10.0",
		"pinia": "^2.2.2",
		"postcss": "^8.4.41",
		"svg-country-flags": "1.2.10",
		"tailwindcss": "^3.4.10",
		"toastify-js": "1.11.1",
		"tsx": "^4.19.0",
		"utf-8-validate": "^6.0.4",
		"uuid": "^10.0.0",
		"vite-express": "^0.19.0",
		"vue": "~3.5.1",
		"vue-i18n": "^9.14.0",
		"vue-router": "^4.4.3",
		"winston": "3.13.1",
		"ws": "^8.18.0",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@types/cookie-parser": "1.4.7",
		"@types/debug": "4.1.12",
		"@types/ejs": "3.1.5",
		"@types/express": "4.17.21",
		"@types/express-session": "^1.17.3",
		"@types/jest": "^29.5.12",
		"@types/lodash-es": "^4.17.12",
		"@types/morgan": "1.9.9",
		"@types/ramda": "0.27.40",
		"@types/supertest": "6.0.2",
		"@types/toastify-js": "^1.12.3",
		"@types/uuid": "8.3.0",
		"@types/ws": "^8.5.12",
		"@types/yargs": "17.0.32",
		"@vitejs/plugin-vue": "^5.1.3",
		"@vue/tsconfig": "^0.5.1",
		"autoprefixer": "^9.8.8",
		"babel-jest": "^29.7.0",
		"esbuild": "^0.23.1",
		"jest": "^29.7.0",
		"lightningcss": "^1.26.0",
		"nodemon": "^3.1.4",
		"postcss-import": "^16.1.0",
		"prettier": "3.3.3",
		"supertest": "6.1.3",
		"ts-jest": "^29.2.5",
		"tsc-alias": "^1.8.10",
		"vite": "^5.4.2",
		"vite-svg-loader": "^5.1.0",
		"vue-tsc": "^2.1.6"
	}
}
