{
	"name": "deemix",
	"version": "0.0.0",
	"private": true,
	"description": "A hybrid monorepo that wraps deemix-webui and lets you use the deemix-js library",
	"main": "index.js",
	"repository": "https://github.com/bambanah/deemix.git",
	"author": "Bambanah",
	"license": "GPL-3.0-only",
	"scripts": {
		"dev": "pnpm -r --parallel --filter=!deemix-gui dev",
		"dev:gui": "pnpm -C gui dev",
		"build": "turbo run build",
		"lint": "turbo run lint",
		"type-check": "turbo run type-check",
		"test": "turbo run test",
		"package": "turbo run package",
		"make": "turbo run make",
		"ci": "turbo run lint type-check build",
		"prepare": "husky"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
		"@commitlint/cli": "^19.4.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@total-typescript/tsconfig": "^1.0.4",
		"@types/node": "^20.16.3",
		"esbuild": "^0.23.1",
		"husky": "^9.1.5",
		"npm-run-all2": "^6.2.0",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"tsx": "^4.19.0",
		"turbo": "^2.1.1"
	},
	"packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
