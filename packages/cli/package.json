{
	"name": "deemix-cli",
	"version": "0.1.0",
	"private": true,
	"description": "A CLI wrapper for deemix",
	"main": "dist/main.cjs",
	"type": "module",
	"bin": {
		"deemix": "dist/main.cjs"
	},
	"scripts": {
		"start": "tsx src/main.ts",
		"build": "tsup src/main.ts --format cjs",
		"compile": "pkg . --out-path out",
		"test": "vitest run",
		"lint": "eslint .",
		"type-check": "tsc --noEmit"
	},
	"pkg": {
		"targets": [
			"node20-macos-x64",
			"node20-macos-arm64",
			"node20-linux-x64",
			"node20-linux-arm64",
			"node20-win-x64",
			"node20-win-arm64"
		]
	},
	"devDependencies": {
		"@yao-pkg/pkg": "^5.15.0",
		"commander": "^12.1.0",
		"deemix": "workspace:*",
		"deezer-sdk": "workspace:*"
	}
}
