{
	"name": "deemix-webui",
	"version": "4.4.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "nodemon",
		"build": "run-s build:*",
		"build:1-client": "vite build",
		"build:2-server": "tsc --build src/server/tsconfig.json",
		"build:3-aliases": "tsc-alias -p src/server/tsconfig.json",
		"build:4-version": "genversion -fe dist/helpers/web-version.js",
		"start": "cross-env NODE_ENV=production node dist/main.js",
		"test": "vitest run",
		"test:watch": "vitest",
		"testlang": "node ./src/client/tests/testlang.js",
		"lint": "eslint .",
		"type-check": "run-p type-check:*",
		"type-check:client": "vue-tsc --noEmit -p src/client/tsconfig.json",
		"type-check:server": "tsc --noEmit -p src/server/tsconfig.json"
	},
	"nodemonConfig": {
		"watch": [
			"src/server",
			"../deemix/dist",
			"../deezer-sdk/dist"
		],
		"ext": "ts,json,js",
		"exec": "tsx --tsconfig ./src/server/tsconfig.json src/server/main.ts",
		"tsconfig": "./src/server/tsconfig.json"
	},
	"exports": {
		".": "./dist/main.js"
	},
	"dependencies": {
		"cookie-parser": "1.4.6",
		"date-fns": "^3.6.0",
		"deemix": "workspace:*",
		"deezer-sdk": "workspace:*",
		"express": "^4.22.1",
		"express-session": "^1.18.2",
		"flag-icon-css": "3.5.0",
		"got": "^14.6.5",
		"lodash-es": "4.17.21",
		"memorystore": "1.6.7",
		"mitt": "^3.0.1",
		"morgan": "1.10.0",
		"pinia": "^2.3.1",
		"postcss": "^8.5.6",
		"svg-country-flags": "1.2.10",
		"tailwindcss": "^3.4.19",
		"toastify-js": "^1.12.0",
		"utf-8-validate": "^6.0.5",
		"uuid": "^10.0.0",
		"vite-express": "^0.19.0",
		"vue": "~3.5.25",
		"vue-i18n": "^10.0.8",
		"vue-router": "^4.6.4",
		"winston": "3.14.2",
		"ws": "^8.18.3",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.10",
		"@types/express": "^4.17.25",
		"@types/express-session": "^1.18.2",
		"@types/lodash-es": "^4.17.12",
		"@types/morgan": "^1.9.10",
		"@types/supertest": "^6.0.3",
		"@types/toastify-js": "^1.12.4",
		"@types/uuid": "^10.0.0",
		"@types/ws": "^8.18.1",
		"@types/yargs": "^17.0.35",
		"@vitejs/plugin-vue": "^5.2.4",
		"autoprefixer": "^10.4.23",
		"cross-env": "^7.0.3",
		"genversion": "^3.2.0",
		"nodemon": "^3.1.11",
		"postcss-import": "^16.1.1",
		"prettier": "^3.7.4",
		"supertest": "^7.1.4",
		"tsc-alias": "^1.8.16",
		"vite": "^5.4.21",
		"vite-svg-loader": "^5.1.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vue-tsc": "^2.2.12"
	}
}
